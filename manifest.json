{
    "manifest_version": 3,
    "name": "Cookie Jar",
    "version": "1.0",
    "description": "Tells you about the cookies stored in the browser",
    "icons": {
        "48": "icons/cookie_48.png",
        "96": "icons/cookie_96.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": ["http://*/*", "https://*/*"],
    "permissions": [ "cookies", "tabs", "activeTab", "scripting"],
    "web_accessible_resources": [{
        "resources": [ "icons/cookie_96.png" ],
        "matches": ["http://*/*", "https://*/*"]
    }
        
    ]
}